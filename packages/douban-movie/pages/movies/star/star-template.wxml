<template name="starTemplate">
  <view class="star-container">
    <view class="star">
      <block wx:for="{{stars}}" wx:for-item="i">
        <image wx:if="{{i}}" class="star-img" src="/images/icon/star.png"></image>
        <image wx:else  class="star-img " src="/images/icon/none-star.png">
        </image>
      </block>
    </view>
    <text class="star-score ">{{score}}</text>
  </view>
</template>